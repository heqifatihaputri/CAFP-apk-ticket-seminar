<%= render 'layouts/shared/breadcrumb' %>
<section class="section">
  <div class="row">
    <div class="col-lg-12">
      <div class="card">
        <div class="card-body">
          <h5 class="card-title">New Event Session</h5>
          <%= render partial: "admin/event_sessions/partials/form",locals:
              { event: @event,
                event_session: @event_session,
                url: admin_event_event_sessions_path(@event) } %>
        </div>
      </div>
    </div>
  </div>
</section>
