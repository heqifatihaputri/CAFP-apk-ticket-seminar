<%= render 'layouts/shared/breadcrumb' %>
<section class="section">
  <div class="row">
    <div class="col-lg-12">
      <div class="card">
        <div class="card-body">
          <h5 class="card-title">New Ticket Stock</h5>
          <%= render partial: "admin/ticket_stocks/partials/form",locals:
              { event: @event,
                stock: @ticket_stock,
                url: admin_event_ticket_stocks_path(@event) } %>
        </div>
      </div>
    </div>
  </div>
</section>
